{
  "name": "theafolayan/100ms-laravel",
  "description": "A Laravel SDK for 100ms API.",
  "type": "library",
  "version": "1.1.0",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "TheAfolayan\\HmsLaravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TheAfolayan\\HmsLaravel\\HmsServiceProvider"
      ]
